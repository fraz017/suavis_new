                                    </div>
                                </div>
                                <div class="control-group">
                                    <div class="controls">
                                    <%= recaptcha_tags callback: 'submitInvisibleRecaptchaForm'%>
                                    </div>
                                </div>
                                <div class="control-group">
                                    <div class="controls">
                                        <button id="send-mail" class="message-btn hidden">Send message</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
